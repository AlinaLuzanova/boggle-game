{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "singleQuote": true,
        "trailingComma": "es5",
        "printWidth": 150,
        "tabWidth": 2,
        "linebreak-style": ["error", "windows"]
    }
}
