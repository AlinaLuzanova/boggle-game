{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "singleQuote": 2,
        "trailingComma": ["error", "es5"],
        "printWidth": [2, 150],
        "tabWidth": [2, 2],
        "linebreak-style": ["error", "windows"]
    }
}
